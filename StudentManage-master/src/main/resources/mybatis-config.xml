        <typeAlias type="per.fly.pojo.Admin" alias="admin"/>
        <typeAlias type="per.fly.pojo.Clazz" alias="clazz"/>
        <typeAlias type="per.fly.pojo.Grade" alias="grade"/>
        <typeAlias type="per.fly.pojo.Student" alias="student"/>
        <typeAlias type="per.fly.pojo.Teacher" alias="teacher"/>
        <typeAlias type="per.fly.pojo.LoginForm" alias="loginForm"/>
    </typeAliases>

</configuration>